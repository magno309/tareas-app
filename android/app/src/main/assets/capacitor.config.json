{
	"appId": "io.ionic.starter",
	"appName": "tareas-app",
	"webDir": "build",
	"bundledWebRuntime": false,
	"plugins": {
		"SplashScreen": {
			"launchShowDuration": 4000,
			"launchAutoHide": true,
			"backgroundColor": "#000",
			"androidScaleType": "CENTER_CROP",
			"showSpinner": true,
			"androidSpinnerStyle": "large",
			"spinnerColor": "#9626e0",
			"splashFullScreen": true,
			"splashImmersive": true
		}
	}
}
